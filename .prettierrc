{
    "printWidth": 140,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": false,
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "tabWidth": 4,
                "printWidth": 250,
                "parser": "json"
            }
        }
    ],
    "plugins": [
        "prettier-plugin-organize-imports"
    ],
    "parser": "typescript"
}
